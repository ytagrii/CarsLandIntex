            <option value="county_name_WASHINGTON">Washington</option>
            <option value="county_name_WEBER">Weber</option>
            <option value="other">Other County</option>
        </select>
    </div>
    <div class="form-group">
        <label asp-for="city">City</label>
        <select class="form-control" asp-for="city">
            <option value="city_AMERICAN_FORK">city_AMERICAN_FORK</option>
            <option value="city_BLUE_CREEK">city_BLUE_CREEK</option>
            <option value="city_BOULDER">city_BOULDER</option>
            <option value="city_EMORY">city_EMORY</option>
            <option value="city_GARDEN_CITY">city_GARDEN_CITY</option>
