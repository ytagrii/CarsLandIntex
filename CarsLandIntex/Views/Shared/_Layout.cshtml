<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    

    <title>@ViewData["Title"] - UT Crash DB</title>
    <link nonce="frick" href="~/css/site.css" rel="stylesheet" />
    <link nonce="frick" rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!--
    NEW STUFF BELOW
    -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link nonce="frick" rel="stylesheet" href="~/css/main.css" />
    <noscript><link nonce="frick" rel="stylesheet" href="~/css/noscript.css" /></noscript>
</head>
<body class="is-preload">
    <header id="header1">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm main_buttons mb-3 nav-position-z">
            <div class="container">
                <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">UT Crash DB</a>
                <div id=pad-case>
                    <div class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                         aria-expanded="false" aria-label="Toggle navigation" id="nav-icon3">
                            <span></span>
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>
                    <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                        <partial name="_LoginPartial" />
                        <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="ExploreData">View Data</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Severity">Severity</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="MachineLearning">ML</a>
                            </li>
                            @if (User.IsInRole("Admin"))
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="AddCrash">Add Crash</a>
                                </li>
                            }
                                
                        </ul>
                    </div>
                </div>
</nav>
    </header>
    <div class="container">
        <!--This enables the cookie consent notification-->
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <br /><br />
    <footer class="border-top footer text-center">
        <div class="container">
            &copy; 2022 - UT Crash DB - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script nonce="frick" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script nonce="frick" src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script nonce="frick" src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    
</body>

</html >